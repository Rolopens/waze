<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="waze.FXMLSignUpViewController">
   <children>
      <Button fx:id="addButton" layoutX="120.0" layoutY="247.0" mnemonicParsing="false" onAction="#addUserButtonPushed" text="Sign Up" />
      <Button fx:id="returnButton" layoutX="315.0" layoutY="247.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" text="Return To Login Screen" />
      <VBox layoutX="42.0" layoutY="40.0" prefHeight="200.0" prefWidth="214.0" spacing="30.0">
         <children>
            <TextField fx:id="Username" promptText="Username" />
            <TextField fx:id="Password" promptText="Password" />
            <TextField fx:id="Email" promptText="Email Address" />
         </children>
      </VBox>
      <VBox layoutX="283.0" layoutY="40.0" prefHeight="200.0" prefWidth="204.0" spacing="30.0">
         <children>
            <TextField fx:id="PhoneNo" promptText="Mobile Number" />
            <TextField fx:id="FirstName" promptText="Last Name" />
            <TextField fx:id="LastName" promptText="First Name" />
         </children>
      </VBox>
   </children>
</AnchorPane>
